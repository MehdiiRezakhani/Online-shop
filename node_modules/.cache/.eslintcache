s from \"./Store.module.css\";\r\n\r\nconst Store = () => {\r\n\r\n    const dispatch = useDispatch();\r\n    const productsState = useSelector(state => state.productsState)\r\n\r\n    useEffect(() => {\r\n       dispatch(fetchProducts())\r\n    }, [])\r\n\r\n    return (\r\n        <div className={styles.container} >\r\n            {\r\n                productsState.loading ? \r\n                <Loader /> :\r\n                productsState.error ?\r\n                    <p>Somethin went wrong</p> :\r\n                    productsState.products.map(product => <Product\r\n                            key={product.id}\r\n                            productData={product}\r\n                        />)\r\n            }\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default Store;","C:\\Users\\asus\\Desktop\\Practice\\148- redux shopping cart - cart\\redux-shopping-cart\\src\\redux\\rootReducer.js",[],"C:\\Users\\asus\\Desktop\\Practice\\148- redux shopping cart - cart\\redux-shopping-cart\\src\\components\\shared\\Loader.js",[],"C:\\Users\\asus\\Desktop\\Practice\\148- redux shopping cart - cart\\redux-shopping-cart\\src\\components\\shared\\Cart.js",[],"C:\\Users\\asus\\Desktop\\Practice\\148- redux shopping cart - cart\\redux-shopping-cart\\src\\components\\shared\\Product.js",[],"C:\\Users\\asus\\Desktop\\Practice\\148- redux shopping cart - cart\\redux-shopping-cart\\src\\redux\\cart\\cartAction.js",[],"C:\\Users\\asus\\Desktop\\Practice\\148- redux shopping cart - cart\\redux-shopping-cart\\src\\redux\\products\\productsAction.js",[],"C:\\Users\\asus\\Desktop\\Practice\\148- redux shopping cart - cart\\redux-shopping-cart\\src\\redux\\products\\productsReducer.js",[],"C:\\Users\\asus\\Desktop\\Practice\\148- redux shopping cart - cart\\redux-shopping-cart\\src\\redux\\cart\\cartReducer.js",[],"C:\\Users\\asus\\Desktop\\Practice\\148- redux shopping cart - cart\\redux-shopping-cart\\src\\helper\\functions.js",[],"C:\\Users\\asus\\Desktop\\Practice\\148- redux shopping cart - cart\\redux-shopping-cart\\src\\components\\shared\\Footer.js",[],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\index.js",[],["147","148"],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\App.js",[],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\components\\ProductDetails.js",["149"],"import React,{useEffect,useState} from 'react';\r\nimport axios from 'axios';\r\nimport {useParams} from 'react-router-dom';\r\nimport { useSelector, useDispatch } from 'react-redux';\r\n\r\n// Style\r\nimport styles from \"./ProductDetails.module.css\";\r\n// Icons\r\nimport trashIcon from \"../assets/icons/trash.svg\"\r\n//redux\r\nimport {addItem, removeItem, increase, decrease} from '../redux/cart/cartAction'\r\n//functions\r\nimport {isInCart, quantityCount} from '../helper/functions';\r\nconst ProductDetails = (props) => {\r\n    const Params = useParams();\r\n    const Pid = Params.id\r\n    const [data,setData] = useState({})\r\n    useEffect(()=>{\r\n           axios.get(`https://fakestoreapi.com/products/${Pid}`)\r\n           .then(response => setData(response.data))\r\n    },[]);\r\n    const {id ,image, title, price, category, description} = data;\r\n    const state = useSelector(state => state.cartState);\r\n    const dispatch = useDispatch();\r\n    return (\r\n        <div>\r\n        {data ? \r\n            <div className={styles.container}>\r\n                <img className={styles.image} src={image} alt=\"product\" />\r\n                <div className={styles.textContainer}>\r\n                    <h3>{title}</h3>\r\n                    <p className={styles.description}>{description}</p>\r\n                    <p className={styles.category}><span>Category:</span> {category}</p>\r\n                    <div className={styles.buttonBox}>\r\n                        <span className={styles.price}>${price}</span>\r\n                        <div className={styles.buttonContainer}>\r\n                            {quantityCount(state, id) === 1 && <button className={styles.smallButton} onClick={() => dispatch(removeItem(data))}><img src={trashIcon} alt=\"trash\" /></button>}\r\n                            {quantityCount(state, id) > 1 && <button className={styles.smallButton} onClick={() => dispatch(decrease(data))}>-</button>}\r\n                            {quantityCount(state, id) > 0 && <span className={styles.counter}>{quantityCount(state, id)}</span>}\r\n                            {\r\n                                isInCart(state, id) ?\r\n                                <button className={styles.smallButton} onClick={() => dispatch(increase(data))}>+</button> :\r\n                                <button onClick={() => dispatch(addItem(data))}>Add to Cart</button>\r\n                            }\r\n                         </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        : <p>Loading....</p> } \r\n        </div>\r\n    );\r\n};\r\n\r\nexport default ProductDetails;","C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\components\\ShopCart.js",[],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\components\\Store.js",["150"],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\components\\shared\\Navbar.js",[],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\components\\shared\\Footer.js",[],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\redux\\store.js",[],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\components\\shared\\Cart.js",[],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\components\\shared\\Product.js",[],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\redux\\rootReducer.js",[],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\components\\shared\\Loader.js",[],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\redux\\cart\\cartAction.js",[],"C:\\Users\\asus\\Desktop\\Practice\\OnlineShop-Redux\\src\\helper\\functions.js"